  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaorbrandao/joaorbrandao.github.io/issues"
  },
  "homepage": "https://github.com/joaorbrandao/joaorbrandao.github.io#readme",
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "autoprefixer": "^9",
    "cross-env": "^5.2.0",
    "laravel-mix": "^4.0.15",
    "postcss": "^7",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.17.2",
    "sass-loader": "^7.1.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "vue-template-compiler": "^2.6.8"
